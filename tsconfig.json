{
  "compilerOptions": {
    "outDir": "bin",
    "sourceMap": true,
    "noImplicitAny": true,
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "lib": [
      "es2015",
      "dom",
      "esnext.asynciterable"
    ],
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": false
  },
  "include": [
    "lib/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
